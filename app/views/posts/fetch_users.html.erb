<table style="width:300px" border ="1" class="table table-striped">
  <thead>
    <td>Email</td>
    <td>Provider</td>
    <td>Role</td>
  </thead>
  <tbody>
    <% @users.each do |user|%>
    <tr>
      <td><%= user.email%></td>
      <td><%= user.provider%></td>
      <td><%= user.role_id%></td>
    </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Posts', posts_path%>